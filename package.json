{
  "name": "excel",
  "version": "1.0.0",
  "description": "Pure JavaScript Excel Application",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "lint": "npx eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TitarenkoBA/Excel.git"
  },
  "keywords": [
    "js",
    "javascript",
    "excel"
  ],
  "author": "Boris Titarenko <titarenkoborisandreevich@yandex.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TitarenkoBA/Excel/issues"
  },
  "homepage": "https://github.com/TitarenkoBA/Excel#readme",
  "private": true,
  "browserslist": "> 0.25%, not dead",
  "jest": {
    "moduleNameMapper": {
      "^@root(.*)$": "<rootDir>/src$1",
      "^@core(.*)$": "<rootDir>/src/core$1",
      "^@components(.*)$": "<rootDir>/src/components$1"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7.3",
    "eslint": "^7.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.2.2",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^13.2.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "eslint-plugin-jest": "^23.20.0",
    "normalize.css": "^8.0.1"
  }
}
